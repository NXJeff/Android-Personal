<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">;

    <TextView
        android:paddingTop="8dp"
        android:paddingBottom="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:text="AAAA"
        android:textSize="16dp"
        android:textColor="@color/blue_grey_700"
        android:fontFamily="@string/card_font_fontFamily_image_text_over"
        android:id="@+id/medical_remark"
        style="@style/card.native.base_simple_title" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/medical_category"
        android:textSize="16sp"
        android:textStyle="normal"
        android:layout_marginLeft="20dp"
        android:singleLine="true"
        android:textColor="#333"
        android:text="DEFAULT" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/medical_last_dismissed_time"
        android:textSize="16sp"
        android:textStyle="normal"
        android:layout_marginLeft="20dp"
        android:singleLine="true"
        android:textColor="#333"
        android:text="DEFAULT" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/medical_next_remind_time"
        android:textSize="16sp"
        android:textStyle="normal"
        android:layout_marginLeft="20dp"
        android:singleLine="true"
        android:textColor="#333"
        android:text="DEFAULT" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/medical_test1"
        android:textSize="16sp"
        android:textStyle="normal"
        android:layout_marginLeft="20dp"
        android:singleLine="true"
        android:textColor="#333"
        android:text="DEFAULT 4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/medical_test2"
        android:textSize="16sp"
        android:textStyle="normal"
        android:layout_marginLeft="20dp"
        android:singleLine="true"
        android:textColor="#333"
        android:text="DEFAULT 4" />

</LinearLayout>